{
  "extends": [
    "development"
  ],
  "hints": {
    "no-inline-styles": "off",
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "-webkit-tap-highlight-color",
          "@-ms-viewport"
        ]
      }
    ]
  },
  "browserslist": [
    "defaults",
    "not ie 11",
    "not and_chr <= 114",
    "not chrome <= 114",
    "not edge <= 114",
    "not firefox <= 113",
    "not safari <= 16.5",
    "not samsung <= 21",
    "not and_ff <= 113",
    "not ios_saf <= 16.5",
    "not opera <= 99"
  ]
}